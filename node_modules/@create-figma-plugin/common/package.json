{
  "name": "@create-figma-plugin/common",
  "version": "1.7.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "Internal utilities for the Create Figma Plugin toolkit",
  "keywords": [
    "create-figma-plugin",
    "figjam",
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/create-figma-plugin.git",
    "directory": "packages/common"
  },
  "type": "module",
  "engines": {
    "node": ">=14"
  },
  "files": [
    "lib"
  ],
  "exports": "./lib/index.js",
  "main": "./lib/index.js",
  "scripts": {
    "build": "yarn run clean && yarn run generate-index-ts && tsc --build",
    "clean": "rm -rf '*.log' lib tsconfig.tsbuildinfo",
    "generate-index-ts": "node --loader tsm ../../scripts/generate-index-ts.ts -- 'src/**/*.ts' && eslint --fix src/index.ts",
    "fix": "eslint --fix '{src,test}/**/*.ts' && prettier --loglevel error --write '*.json' 'test/**/fixtures/**/*.json'",
    "lint": "eslint '{src,test}/**/*.ts'",
    "test": "yarn run build && ava 'test/**/*.ts'"
  },
  "dependencies": {
    "@sindresorhus/slugify": "2.1.0",
    "fs-extra": "^10.0.0",
    "kleur": "^4.1.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.11.6",
    "ava": "^3.15.0",
    "eslint": "^8.1.0",
    "eslint-config-yuanqing": "^0.0.6",
    "prettier": "^2.4.1",
    "tsm": "^2.1.4",
    "typescript": "^4.4.4"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader",
      "tsm"
    ],
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "serial": true,
    "timeout": "2m"
  },
  "eslintConfig": {
    "extends": "eslint-config-yuanqing"
  },
  "prettier": "eslint-config-yuanqing/prettier",
  "gitHead": "504f62c9f5fefee42bd7337cbc036ada29546355"
}
