{
  "dependencies": {
    "@create-figma-plugin/utilities": "^1.7.2",
    "preact": "^10.6.4"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^1.7.2",
    "@create-figma-plugin/tsconfig": "^1.7.2",
    "@figma/plugin-typings": "^1.40.0",
    "@figma/widget-typings": "1.0.3",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "watch": "build-figma-plugin --typecheck --watch"
  },
  "figma-plugin": {
    "editorType": [
      "figjam"
    ],
    "permissions": [
      "currentuser",
      "activeusers"
    ],
    "containsWidget": true,
    "id": "scrumalot",
    "name": "Scrumalot",
    "main": "src/main.tsx",
    "ui": "src/ui.tsx"
  }
}
